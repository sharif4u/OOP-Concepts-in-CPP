//Encapsulation
#include<bits/stdc++.h>
using namespace std;
class Student{
private:
    int marks;
public:
    void setMarks(int m){
        marks = m;
    }
    int getMarks(){
        return marks;
    }
};

int main(){

    Student s1;
    s1.setMarks(97);
    cout << "Marks of the student " << s1.getMarks() << endl;
    return 0;
}
